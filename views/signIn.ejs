


<!-- sign in form  -->
<form action="/users/createSession" method="POST">
    <h1>Sign In</h1>
    <input type="email" name="email" placeholder="Email*" required>
    <input type="password" name="password" placeholder="Password*" required>
    <button value="sign in" class="submit">Sign In</button>
</form>

<!-- if not Registered button  to go to sign up page -->
<p>Not Registered?</p>
<a href="/users/signUp"><button class="submit">Sign Up</button></a>
